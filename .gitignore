import React from 'react';

// Simplified icon components based on the FB aesthetic
const SearchIcon = () => (
    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" strokeWidth={2.5} stroke="currentColor" className="w-5 h-5 text-fb-gray-dark">
        <path strokeLinecap="round" strokeLinejoin="round" d="m21 21-5.197-5.197m0 0A7.5 7.5 0 1 0 5.197 5.197a7.5 7.5 0 1 0 10.607 10.607Z" />
    </svg>
);
const HomeIcon = ({ isActive = false }: { isActive?: boolean }) => (
    <svg xmlns="http://www.w3.org/2000/svg" fill={isActive ? '#1877f2' : 'none'} viewBox="0 0 24 24" strokeWidth={isActive ? 0 : 1.5} stroke={isActive ? 'none' : '#65676b'} className={`w-7 h-7 ${isActive ? 'text-fb-blue' : 'text-fb-gray-dark'}`}>
        <path strokeLinecap="round" strokeLinejoin="round" d="m2.25 12 8.954-8.955c.44-.439 1.152-.439 1.591 0L21.75 12M4.5 9.75v10.125c0 .621.504 1.125 1.125 1.125h9.75a1.125 1.125 0 0 0 1.125-1.125V9.75M8.25 21h8.25" />
    </svg>
);


const Header: React.FC = () => {
  return (
    <header className="fixed top-0 left-0 right-0 h-14 bg-white shadow-md z-50 flex items-center justify-between px-4">
      {/* Left Section: Logo & Search */}
      <div className="flex items-center space-x-2">
        {/* Facebook Logo Placeholder */}
        <div className="text-3xl font-bold text-fb-blue mr-1">f</div>
        <div className="hidden lg:flex bg-fb-gray-light p-2 rounded-full items-center space-x-2 w-64">
          <SearchIcon />
          <input 
            type="text" 
            placeholder="Search Facebook" 
            className="bg-transparent focus:outline-none text-sm w-full placeholder:text-fb-gray-dark"
          />
        </div>
      </div>

      {/* Middle Section: Navigation Icons */}
      <nav className="flex items-center h-full flex-grow justify-center max-w-xl">
        <div className="flex h-full w-full">
          <div className="flex items-center px-6 border-b-4 border-fb-blue cursor-pointer transition duration-150 ease-in-out flex-1 justify-center">
            <HomeIcon isActive={true} />
          </div>
          <div className="flex items-center px-6 cursor-pointer rounded-lg transition duration-150 ease-in-out flex-1 justify-center hover:bg-fb-gray-light">
            <HomeIcon isActive={false} /> 
          </div>
          <div className="hidden md:flex items-center px-6 cursor-pointer rounded-lg transition duration-150 ease-in-out flex-1 justify-center hover:bg-fb-gray-light">
            <HomeIcon isActive={false} />
          </div>
        </div>
      </nav>

      {/* Right Section: User & Actions */}
      <div className="flex items-center space-x-2 md:space-x-4">
        <div className="hidden md:block w-10 h-10 bg-fb-gray-light rounded-full flex items-center justify-center cursor-pointer hover:bg-gray-300">
            {/* Menu Icon Placeholder */}
            <span className="text-xl">üí¨</span>
        </div>
        <div className="w-10 h-10 bg-fb-gray-light rounded-full flex items-center justify-center cursor-pointer hover:bg-gray-300">
            {/* Notifications Icon Placeholder */}
            <span className="text-xl">üîî</span>
        </div>
        
        {/* Profile Avatar */}
        <div className="w-10 h-10 bg-gray-400 rounded-full cursor-pointer overflow-hidden">
            {/* User Profile Image */}
            <img src="/vite.svg" alt="Profile" className="w-full h-full object-cover"/>
        </div>
      </div>
    </header>
  );
};

export default Header;